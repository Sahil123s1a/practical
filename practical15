#include <iostream>
#include <vector>
#include <climits>
using namespace std;

int n;
vector<vector<int>> graph;
vector<bool> visited;
int minTime = INT_MAX;

void tsp(int curr, int count, int cost) {
    if(count == n) {
        minTime = min(minTime, cost);
        return;
    }
    for(int next = 0; next < n; next++) {
        if(!visited[next] && graph[curr][next] != 0) {
            visited[next] = true;
            tsp(next, count + 1, cost + graph[curr][next]);
            visited[next] = false;
        }
    }
}

int main() {
    cin >> n;
    graph.resize(n, vector<int>(n));
    visited.resize(n);

    for(int i=0; i<n; i++)
        for(int j=0; j<n; j++)
            cin >> graph[i][j];

    visited[0] = true; // start from location 0
    tsp(0, 1, 0);

    cout << "Minimum delivery time: " << minTime << "\n";
}
