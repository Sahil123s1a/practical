#include <iostream>
using namespace std;
#define SIZE 5

class CircularQueue {
    string events[SIZE];
    int front = -1, rear = -1;
public:
    bool isFull() { return (front == 0 && rear == SIZE-1) || (rear + 1 == front); }
    bool isEmpty() { return front == -1; }

    void addEvent(string e) {
        if (isFull()) cout << "Queue Full!\n";
        else {
            if (front == -1) front = 0;
            rear = (rear + 1) % SIZE;
            events[rear] = e;
            cout << "Added: " << e << endl;
        }
    }

    void processEvent() {
        if (isEmpty()) cout << "No events to process!\n";
        else {
            cout << "Processed: " << events[front] << endl;
            if (front == rear) front = rear = -1;
            else front = (front + 1) % SIZE;
        }
    }

    void display() {
        if (isEmpty()) cout << "No pending events!\n";
        else {
            cout << "Pending: ";
            int i = front;
            while (true) {
                cout << events[i] << " ";
                if (i == rear) break;
                i = (i + 1) % SIZE;
            }
            cout << endl;
        }
    }
};

int main() {
    CircularQueue cq;
    cq.addEvent("E1");
    cq.addEvent("E2");
    cq.addEvent("E3");
    cq.display();
    cq.processEvent();
    cq.display();
}
