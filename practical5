salaries = [45000.5, 52000.0, 31000.75, 60000.25, 48000.0, 75000.5, 90000.0]

for i in range(len(salaries)-1):
    for j in range(len(salaries)-i-1):
        if salaries[j] > salaries[j+1]:
            salaries[j], salaries[j+1] = salaries[j+1], salaries[j]

print("Sorted Salaries:", salaries)
print("Top 5 Salaries:", salaries[-5:][::-1])
